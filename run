def dump_public():
	try:
		os.mkdir('dump')
	except:pass
	try:
		xaco = input(f"{garis} Masukan Id Public : {K}")
		cuy = input(f"{garis} Nama File Nya : {K} ")
		bangsat  = ('dump/' + cuy + '.json').replace(' ', '_')
		yandek = open(bangsat, 'w')
		token = open('token.txt','r').read()
		for fuck in requests.get(f"https://graph.facebook.com/{xaco}?fields=id,friends&access_token={token}").json()['friends']['data']:
			try:
			    if len(fuck['id'])==7 or len(fuck['id'])==8 or len(fuck['id'])==9 or len(fuck['id'])==10:
			        pass
			    else:
			        id.append(fuck['id']+'|'+fuck['name'])
			        yandek.write(fuck['id']+'|'+fuck['name'] + '\n')
			except:continue
			fr = random.choice([O,B,H,P,K,M,U])
			sys.stdout.write(f'\r {Z}[{K}â€¢{Z}]{fr} %s {P}â€¢ {fr}%s                  \r\n\x1b[38;5;231m [ \x1b[38;5;46m%s\x1b[38;5;231m ] [ \x1b[38;5;46m%s\x1b[38;5;231m ] {P}Proses Dump...'%(fuck['id'],fuck['name'],datetime.datetime.now().strftime('%H:%M:%S'), len(id))); sys.stdout.flush()
			time.sleep(0.0050)
			
		yandek.close()
		print('')
		jalan(f"\n{garis} Target Berhasil Dikumpulkan...");print(f"{garis} File Tersimpan Difolder {H}dump\n{garis} {bangsat}")
		input(f"{garis} Enter Untuk Kembali... ");menu()
	except (KeyError,IOError):
		os.remove(wkwk)
		jalan(f"{garis} Dump Id Gagal, Kemungkinan Target Tidak Public.\n")
		input(f"{garis} Enter Untuk Kembali... ");menu()
		
def dump_followers():
	try:
		os.mkdir('dump')
	except:pass
	try:
		xaco = input(f"{garis} id followers  :\x1b[38;5;46m ")
		cuy = input(f"{garis} nama file  :\x1b[38;5;46m ")
		print("")
		bangsat  = ('dump/' + cuy + '.json').replace(' ', '_')
		yandek = open(bangsat, 'w')
		token = open('token.txt','r').read()
		cyna = requests.get(f'https://graph.facebook.com/{xaco}?fields=id,subscribers&access_token=%s'%(token)).json()
		for fuck in cyna['subscribers']['data']:
			id.append(fuck['id']+'|'+fuck['name'])
			yandek.write(fuck['id']+'|'+fuck['name'] + '\n')
			fr = random.choice([O,B,H,P,K,M,U])
			sys.stdout.write(f'\r\033[0m *--> %s {fr}â€¢ \033[0m%s                  \r\n\x1b[38;5;231m [ \x1b[38;5;46m%s\x1b[38;5;231m ] [ \x1b[38;5;46m%s\x1b[38;5;231m ] \x1b[0;96mproses dump ID...'%(fuck['id'],fuck['name'],datetime.datetime.now().strftime('%H:%M:%S'), len(id)
			)); sys.stdout.flush()
			time.sleep(0.0050)
			
		yandek.close()
		print("")
		jalan(f"\n{garis} berhasil dump id followers ");print(f"{garis} salin output file *--> [ %s%s%s ]"%(H,bangsat,P))
		input(f"{garis} kembali ");menu()
	except (KeyError,IOError):
		jalan(f"{garis} gagal dump id, kemungkinan id tidak ada.\n")
		input(f"{garis} kembali ");menu()

def dump_post():
	try:os.mkdir('dump')
	except:pass
	try:
		token = open('token.txt','r').read()
	except (KeyError,IOError):
		jalan(garis+" token kadaluarsa");cek_cookie()
	try:
		xaco = input(f"{garis} id likes  :\x1b[38;5;46m ")
		cuy = input(f"{garis} nama file  :\x1b[38;5;46m ")
		limit = input(f"{garis} limit likes : {H}")
		print("")
		bangsat  = ('dump/' + cuy + '.json').replace(' ', '_')
		yandek = open(bangsat, 'w')
		cyna = requests.get(f'https://graph.facebook.com/{xaco}?fields=likes&access_token=%s'%(token)).json()
		for fuck in cyna['likes']['data']:
			id.append(fuck['id']+'|'+fuck['name'])
			yandek.write(fuck['id']+'|'+fuck['name'] + '\n')
			fr = random.choice([O,B,H,P,K,M,U])
			sys.stdout.write(f'\r\033[0m *--> %s {fr}â€¢ \033[0m%s                  \r\n\x1b[38;5;231m [ \x1b[38;5;46m%s\x1b[38;5;231m ] [ \x1b[38;5;46m%s\x1b[38;5;231m ] \x1b[0;96mproses dump ID...'%(fuck['id'],fuck['name'],datetime.datetime.now().strftime('%H:%M:%S'), len(id)
			)); sys.stdout.flush()
			time.sleep(0.0050)
			
		yandek.close()
		jalan(f"\n{garis} berhasil dump id post likes ");print(f"{garis} salin output file *--> [ %s%s%s ]"%(H,bangsat,P))
		input(f"{garis} kembali ");menu()
	except (KeyError,IOError):
		os.remove(wkwk)
		jalan(f"{garis} gagal dump id, kemungkinan id tidak ada.\n")
		input(f"{garis} kembali ");menu()

def dump_publicv2():
	kuntol=0
	tutlu=[]
	try:
		token = open('token.txt','r').read()
	except (KeyError,IOError):
		jalan(garis+" token kadaluarsa");cek_cookie()
	it = input(f"{garis} id target :"+H+" ")
	try:
		ka = requests.get(f'https://graph.facebook.com/{it}?fields=id,friends&access_token=%s'%(token))
		oioo = json.loads(ka.text)
		for fuck in oioo['friends']['data']:
			tutlu.append(fuck['id'])
		cyna = requests.get('https://graph.facebook.com/%s?access_token=%s'%(it,token)).json()
		print(f"{garis} name : %s%s"%(H,cyna['name']))
		print(f"{garis} total friends : {H}{len(tutlu)}")
	except (KeyError,IOError):
		jalan(f"{garis} cookie kadaluarsa")
		login()
	tl=[]
	to=[]
	ttl_tar=[]
	tlo=[]
	idl=[]
	lim = input(f"{garis} limit dump id :"+H+" ")
	ka = requests.get(f'https://graph.facebook.com/{it}?fields=id,friends&access_token=%s'%(token)) 
	oioo = json.loads(ka.text)
	for fuck in oioo['friends']['data']:
		tl.append(fuck['id'])
	print("")
	x=f"{P2}[01] id urutan new\n{P2}[02] id urutan old\n{P2}[03] id urutan random"
	RakaXF(panel(x,style=f"{H3}"))
	GlukTzy = input(garis+" pilih : "+H)
	if GlukTzy in ['2','02']:
		for bacot in tl:
			idl.append(bacot)
	elif GlukTzy in ['1','01']:
		for bacot in tl:
			idl.insert(0,bacot)
	elif GlukTzy in ['3','03']:
		for bacot in tl:
			xx = random.randint(0,len(idl))
			idl.insert(xx,bacot)
	else:
		jalan(garis+" isi yang benar")
		exit()
	print("")
	for uik in idl:
		try:
			eil = json.loads(ka.text)
			try:
				for fuck in eil['friends']['data']:
					to.append(fuck['id']+'|'+fuck['name'])
			except KeyError:
				continue
			kuntol+=1
			print(f"\r{P} [{H}{str(kuntol)}{P}] {uik}|{len(to)}")
			to.clear()
		except KeyError:
			print(K+'*--> '+M+'akun terspam')
	print("")
	x=f"{P2}salin hasil nya cokk biar gk ulang dump lagi!!"
	RakaXF(panel(x,style=f"{H3}"))
	input(f"{garis} enter untuk kembali")
	menu()

